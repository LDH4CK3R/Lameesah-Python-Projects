   ],
   "source": [
    "email = input(\"Enter your email: \")\n",
    "\n",
    "index = email.index(\"@\")\n",
    "\n",
    "username = email[:index]\n",
    "domain = email[index + 1:]\n",
    "\n",
    "print(f\"Your username is {username} and domain is {domain}\")"
   ]
  }
